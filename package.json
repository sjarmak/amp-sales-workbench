{
  "name": "amp-sales-workbench",
  "version": "0.1.0",
  "description": "Multi-agent sales workflow system connecting Gong, Notion, and Salesforce via Amp SDK",
  "type": "module",
  "engines": {
    "node": ">=18.19.0"
  },
  "scripts": {
    "start": "tsx src/execute-agent.ts",
    "dev": "DEBUG=1 tsx src/execute-agent.ts",
    "manage": "tsx src/execute-agent.ts",
    "precall": "tsx scripts/test-precall-brief.ts",
    "postcall": "tsx scripts/test-postcall.ts",
    "followup": "tsx scripts/test-followup-email.ts",
    "demo": "tsx scripts/test-demo-idea.ts",
    "coaching": "tsx scripts/test-coaching.ts",
    "handoff": "tsx scripts/test-handoff.ts",
    "closed-lost": "tsx scripts/test-closed-lost.ts",
    "backfill": "tsx scripts/test-backfill.ts",
    "risks": "tsx scripts/test-risk-heuristics.ts",
    "meeting-summary": "tsx scripts/test-meeting-summary.ts",
    "test:agents": "tsx scripts/test-all-agents.ts",
    "gong:backfill": "tsx scripts/gong-cache-backfill.ts",
    "gong:stats": "tsx scripts/gong-cache-stats.ts",
    "gong:test": "tsx scripts/gong-cache-backfill.ts --test",
    "api": "tsx api-server.ts",
    "web": "cd web && npm run dev",
    "start:web": "concurrently \"npm run api\" \"npm run web\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "amp-sdk",
    "sales",
    "crm",
    "gong",
    "notion",
    "salesforce",
    "workflow"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@sourcegraph/amp-sdk": "^0.1.0-20251014160341-g7c5de51",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "cheerio": "^1.1.2",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "turndown": "^7.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/turndown": "^5.0.5",
    "tsx": "^4.16.0",
    "typescript": "^5.6.3"
  }
}
